#include "stdafx.h"
#include <cmath>
#include <iostream>

using namespace std;
int main()
{
	int x1, x2, y1, y2, r;
	cout << "Vvedite coordinaty x1, y1, x2, y2" << endl;
	cin >> x1;
	cin >> y1;
	cin >> x2;
	cin >> y2;
	cout << sqrt((x2 - x1)*(x2 - x1) + (y2 - y1)*(y2 - y1));
	system("pause");
	return 0;
}
